#include <iostream>
#include <cmath> 

using namespace std;

int main() {
    double num, root;

    cout << "Enter a number: ";
    cin >> num;

    if (num < 0) {
        cout << "Invalid input. Cannot find square root of negative number." << endl;
    } else {
        root = sqrt(num);
        cout << "The square root of " << num << " is " << root << endl;
    }

    return 0;
}
